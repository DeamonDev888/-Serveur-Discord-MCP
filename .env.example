# Token Discord du bot (OBTENEZ UN NOUVEAU TOKEN - l'ancien est compromis)
DISCORD_TOKEN=VOTRE_NOUVEAU_TOKEN_ICI

# ID utilisateur admin (pour les commandes admin)
ADMIN_USER_ID=

# Configuration du bot
BOT_PREFIX=!
BOT_ACTIVITY=Playing with MCP

# Limites de fichiers (en octets)
MAX_FILE_SIZE_IMAGE=26214400
MAX_FILE_SIZE_VIDEO=104857600
